{
  "name": "NetworkManager",
  "version": "0.9.0",
  "main": "./main.js",
  "author": {
    "name": "Rodrigo Felipe dos Santos Lima",
    "email": "rodrigo.suporte@contas.com"
  },
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron-builder": "^25.0.5",
    "react": "^18.3.1",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-pro-sidebar": "^1.1.0",
    "react-router-dom": "^6.26.2",
    "react-scripts": "5.0.1",
    "react-spinner": "^0.2.7",
    "react-use-websocket": "^4.9.0",
    "styled-components": "^6.1.13",
    "sweetalert": "^2.1.2"
  },
  "devDependencies": {
    "electron": "^32.1.0",
    "electron-builder": "^25.0.5"
  },
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "concurrently \"cross-env BROWSER=none react-scripts start\" \"npm run server\" \"npm run wait-react\"",
    "wait-react": "node preload.js && npm run electron",
    "build": "react-scripts build",
    "server": "cd .. && nodemon src/app.js",
    "electron": "electron .",
    "electron-build": "electron-builder "
  },
  "build": {
    "appId": "com.ManageNetwork",
    "productName": "Network Manager",
    "files": [
      "./main.js",
      "./preload.js",
      "./node_modules/**/*"
    ],
    "directories": {
      "output": "C:/Program Files/TI Administration/"
    },
    "extraMetadata": {
      "main": "./main.js"
    },
    "win": {
      "icon": "C:/Program Files/TI Administration/logo.ico",
      "target": "portable"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
